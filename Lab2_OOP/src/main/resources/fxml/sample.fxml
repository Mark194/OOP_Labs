<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Label layoutX="115.0" layoutY="23.0" prefHeight="79.0" prefWidth="407.0" text="Выберите программу">
         <font>
            <Font name="Times New Roman" size="42.0" />
         </font>
      </Label>
      <Pane fx:id="paneEditor" onMouseClicked="#loadGraphicsEditor" layoutX="26.0" layoutY="111.0">
         <children>
            <ImageView fitHeight="113.0" fitWidth="125.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/graphic.png" />
               </image>
            </ImageView>
            <Label layoutX="146.0" layoutY="17.0" prefHeight="79.0" prefWidth="292.0" text="Графический редактор">
               <font>
                  <Font name="Times New Roman" size="29.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Pane fx:id="paneProc" layoutX="25.0" layoutY="226.0">
         <children>
            <ImageView fitHeight="113.0" fitWidth="125.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/processor.png" />
               </image>
            </ImageView>
            <Label layoutX="147.0" layoutY="17.0" prefHeight="79.0" prefWidth="292.0" text="Микропроцессор">
               <font>
                  <Font name="Times New Roman" size="29.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </children>
</AnchorPane>
